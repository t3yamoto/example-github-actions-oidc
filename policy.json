{
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Principal": {
        "Federated": "arn:aws:iam::450677052036:oidc-provider/token.actions.githubusercontent.com"
      },
      "Condition": {
        "StringEquals": {
          "token.actions.githubusercontent.com:aud": "https://github.com/t3yamoto/example-github-actions-oidc",
          "token.actions.githubusercontent.com:sub": "repo:t3yamoto/example-github-actions-oidc:*"
        }
      }
    }
  ]
}
